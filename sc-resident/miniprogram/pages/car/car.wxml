<view class="weui-cells weui-cells_after-title">
	<navigator url="/pages/car/index" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
		<view class="weui-cell__hd">
			<image src="/images/car.png" style="margin-right: 35px;vertical-align: middle;width:40px; height:40px;"></image>
		</view>
		<view class="weui-cell__bd">我的拼车信息</view>
		<view class="weui-cell__ft weui-cell__ft_in-access"></view>
	</navigator>
	<navigator url="/pages/car/signin" class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active">
		<view class="weui-cell__hd">
			<image src="/images/mycar.png" style="margin-right: 35px;vertical-align: middle;width:40px; height: 40px;"></image>
		</view>
		<view class="weui-cell__bd">发起拼车</view>
		<view class="weui-cell__ft weui-cell__ft_in-access"></view>
	</navigator>
	<navigator url="/pages/car/seek" class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active">
		<view class="weui-cell__hd">
			<image src="/images/list.png" style="margin-right: 35px;vertical-align: middle;width:40px; height: 40px;"></image>
		</view>
		<view class="weui-cell__bd">查找拼车信息</view>
		<view class="weui-cell__ft weui-cell__ft_in-access"></view>
	</navigator>
</view>