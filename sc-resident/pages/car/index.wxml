<!--pages/car/index.wxml-->
<view class="weui-cells weui-cells_after-title">
	<navigator url="/pages/car/history?carpoolStatus=complete" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
		<view class="weui-cell__hd">
			<image src="/images/history.png" style="margin-right: 35px;vertical-align: middle;width:40px; height:40px;"></image>
		</view>
		<view class="weui-cell__bd">历史拼车记录</view>
		<view class="weui-cell__ft weui-cell__ft_in-access"></view>
	</navigator>
	<navigator url="/pages/car/own?carpoolStatus=in_progress" class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active">
		<view class="weui-cell__hd">
			<image src="/images/now.png" style="margin-right: 35px;vertical-align: middle;width:40px; height: 40px;"></image>
		</view>
		<view class="weui-cell__bd">预约拼车记录</view>
		<view class="weui-cell__ft weui-cell__ft_in-access"></view>
	</navigator>
</view>