page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.head{
  height: 125px;
  width: 100%;
  background: #fff;
  
}
.avatar{
  height: 125px;
  width: 125px;
}
.image{
  margin-top: 13px;
  margin-left: 10px;
  height:100px;
  width: 100px;
  border-radius: 50%;
}
.nickname{
  margin-top: -80px;
  margin-left: 60px;
  height: 100px;
  width: 125px;
}

.menuitems{
  width: 100%;
  margin-top: 20px;
  background: #fff;
}

/* 横着的分割View样式 */
/*分割线样式*/
.div-line{
  background: #E0E3DA;
  width: 100%;
  height: 3rpx;
}
.list-item {
  width: 100%;
  height: 120rpx;
}
.list-item_active {
  background-color: #E0E3DA;
}
.item {
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 120rpx;
}
.item-text {
  position: fixed;
  left: 80rpx;
  font-size: 38rpx;
}
.item-array {
  position: fixed;
  right: 50rpx;
  font-size: 38rpx;
}

.item-image {
  position: fixed;
  left: 25rpx;
  width: 40rpx;
  height: 40rpx;
  border-radius: 0px;
}
